            String select = "A.nom, B.id_producte, C.nom,C.quantitat from proveidor A, porta B, producte C where B.id_proveidor=A.id and B.id_producte=C.id";
    
                //obtenim la nota minima
                double minim0;
                int iminim=0;
        
                int minim;
                Object prod2Object;
                int prod;
                for (int i=0;i<prod;i++){
                   int[] prov;
                if (prov[i]<minim){
                        int[] valor;
                        minim=valor[i];
                        iminim= i;
                   }
                }
            
                String[] prov;
                System.out.println("\n" + prov[iminim]+" valor minim: "+minim);
        
        
                //obtenim la nota maxima
                double maxim=0;
