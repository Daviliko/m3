                int prod2;
                for (int i=0;i<prod2;i++){
                    double[] prod1;
                    if (prod1[i]>maxim){
                        double[] valor;
                        maxim=(int) valor[i];
                        imaxim= i;
                    }
                } 
        
                System.out.println(prov[imaxim]+" valor maxim: "+maxim);
        
                    //obtenim la nota mitjana
